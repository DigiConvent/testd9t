      "@tailwindcss/vite": "^4.0.9",
      "@vueuse/core": "^13.0.0",
      "pinia": "^2.1.7",
      "primeicons": "^7.0.0",
      "primevue": "^4.2.5",
      "tailwindcss": "^4.0.9",
      "valibot": "^1.0.0-rc.3",
      "vee-validate": "^4.15.0",
      "vue": "^3.4.29",
      "vue-i18n": "^10.0.5",
      "vue-router": "^4.3.3"
   },
   "devDependencies": {
