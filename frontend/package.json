      "@vue/eslint-config-typescript": "^13.0.0",
      "@vue/test-utils": "^2.4.6",
      "@vue/tsconfig": "^0.5.1",
      "eslint": "^8.57.1",
      "eslint-config-prettier": "^10.0.2",
      "eslint-plugin-vue": "^9.32.0",
      "globals": "^16.0.0",
      "jsdom": "^24.1.0",
      "npm-run-all2": "^6.2.0",
      "prettier": "^3.2.5",
      "prettier-eslint": "^16.3.0",
      "typescript": "~5.4.0",
      "typescript-eslint": "^8.26.0",
